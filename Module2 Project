#include <iostream>
#include <string>
using namespace std;

int main() {
    string string1, string2, print_result;

    for (int i = 1; i <= 3; i++) {
        cout << "Attempt " << i << " - Enter the first string: ";
        getline(cin, string1);

        cout << "Attempt " << i << " - Enter the second string: ";
        getline(cin, string2);

        print_result = string1 + " " + string2;

        cout << "Result is: " << print_result << "\n\n";
    }

    return 0;
}

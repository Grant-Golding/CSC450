#include <iostream>
#include <string>

int main() {
    // Variables for Personal Info
    std::string firstName = "Grant";
    std::string lastName = "Golding";
    std::string streetAddress = "203 W carlson";
    std::string city = "Cheyenne";
    std::string zipCode = "82009";

    // Print to Console
    std::cout << "First Name: " << firstName << std::endl;
    std::cout << "Last Name: " << lastName << std::endl;
    std::cout << "Street Address: " << streetAddress << std::endl;
    std::cout << "City: " << city << std::endl;
    std::cout << "Zip Code: " << zipCode << std::endl;

    return 0;
}
